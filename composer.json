{
  "name": "usepack/iiko-cloud-sdk",
  "description": "PHP SDK для iikoCloud API v1.0",
  "keywords": [
    "iiko",
    "iikoCloud",
    "iiko cloud",
    "iiko integration",
    "iiko sdk"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Svyatoslav Fyodorov",
      "email": "intelrug@gmail.com",
      "role": "Creator"
    }
  ],
  "require": {
    "php": "^8.0",
    "ext-json": "*",
    "guzzlehttp/guzzle": "^7.3"
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^3.13"
  },
  "config": {
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "UsePack\\IikoCloud\\": "src/"
    }
  },
  "support": {
    "issues": "https://github.com/usepack/iiko-cloud-sdk/issues",
    "docs": "https://github.com/usepack/iiko-cloud-sdk/blob/master/README.md"
  }
}
